<?xml version="1.0" ?>
<rules max_depth="1000">
  <rule name="entry">
    <call transforms="" rule="mondrian"/>
  </rule>
  <rule name="mondrian">
    <call transforms="" rule="a2"/>
    <call transforms="" rule="[['x', '-0.5'], ['z', '-0.5'], ['ry', '90']]"/>
    <call transforms="" rule="[['x', '+0.5'], ['z', '-0.5'], ['ry', '90']]"/>
    <call transforms="" rule="[['z', '-1']]"/>
    <call transforms="" rule="[['y', '+0.5'], ['z', '-0.5'], ['rx', '90']]"/>
    <call transforms="" rule="[['y', '-0.5'], ['z', '-0.5'], ['rx', '90']]"/>
  </rule>
  <rule name="a2" weight="2" max_depth="2" successor="d">
    <call transforms="" rule="[['s', '0.333', '1', '1'], ['x', '-1']]"/>
    <call transforms="" rule="[['s', '0.666', '1', '1'], ['x', '0.26']]"/>
  </rule>
  <rule name="a2" weight="2" max_depth="2" successor="d">
    <call transforms="" rule="[['s', '1', '0.333', '1'], ['y', '-1']]"/>
    <call transforms="" rule="[['s', '1', '0.666', '1'], ['y', '0.26']]"/>
  </rule>
  <rule name="a2">
    <call transforms="" rule="d"/>
  </rule>
  <rule name="d">
    <call transforms="" rule="[['s', '1', '1', '0.02'], 'color', '#F00']"/>
  </rule>
  <rule name="d">
    <call transforms="" rule="[['s', '1', '1', '0.02'], 'color', '#0404A2']"/>
  </rule>
  <rule name="d">
    <call transforms="" rule="[['s', '1', '1', '0.02'], 'color', '#FFFE33']"/>
  </rule>
  <rule name="d" weight="2">
    <call transforms="" rule="[['s', '1', '1', '0.02'], 'color', '#FFF']"/>
  </rule>
  <rule name="square">
    <instance transforms="" shape="box"/>
    <instance transforms="" shape="box"/>
    <instance transforms="" shape="box"/>
    <instance transforms="" shape="box"/>
    <instance transforms="" shape="box"/>
  </rule>
</rules>
